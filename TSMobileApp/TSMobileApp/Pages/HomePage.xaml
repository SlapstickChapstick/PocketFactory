<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="TSMobileApp.Pages.HomePage">
    <ContentPage.Content>
        <StackLayout>
            <Frame BackgroundColor="{x:DynamicResource ShellColor}">
                <Label Text="Home" TextColor="White" FontSize="18"/>
            </Frame>
            <StackLayout Margin="25">
                <Button x:Name="Logout_Btn" Text="Logout" BackgroundColor="{x:DynamicResource ShellColor}" TextColor="White" Clicked="Logout_Btn_Clicked"/>
            </StackLayout>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>